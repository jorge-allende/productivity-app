# Copy this file to .env.local and fill in your values

# Convex Configuration
REACT_APP_CONVEX_URL=https://your-instance.convex.cloud

# Auth0 Configuration  
REACT_APP_AUTH0_DOMAIN=your-domain.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your-client-id
REACT_APP_AUTH0_AUDIENCE=https://your-app.com/api

# Development Settings
# Set to true to bypass authentication in development
REACT_APP_DEV_MODE=true
REACT_APP_BYPASS_AUTH=true

# Optional: Different environments
# REACT_APP_ENV=development|staging|production